<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="16dp"
    android:background="@color/olive_super_light_yellow"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/display_ach"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/olive_dark_green"
        android:gravity="center"
        android:text="@string/cam_display"
        android:textAllCaps="true"
        android:textColor="@color/white"
        android:textSize="25sp"
        android:textStyle="bold" />


    <ImageView
        android:id="@+id/image1"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@color/black"
        android:cropToPadding="true"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toTopOf="@id/hguideline"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageButton
        android:id="@+id/icon1"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_camera"
        android:backgroundTint="@color/olive_light_green_2"
        app:layout_constraintBottom_toTopOf="@id/hguideline"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/image1"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="639dp" />

    <androidx.constraintlayout.widget.Group
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="image1,icon1"
        android:id="@+id/group1"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/hguideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.59" />

    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/vguideline"
        app:layout_constraintGuide_percent="0.6"
        android:orientation="vertical"
        />

    <TextView
        android:id="@+id/textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/hguideline"
        app:layout_constraintBottom_toTopOf="@id/image3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/vguideline"
        android:layout_marginStart="20dp"
        app:layout_constraintHorizontal_bias="0.1"
        tools:text="Content to read from file!"
        />

    <Button
        android:id="@+id/button"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/hguideline"
        app:layout_constraintStart_toStartOf="@id/vguideline"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/image3"
        android:text="reveal"
        android:textSize="14sp"
        android:textColor="@color/white"
        android:backgroundTint="@color/olive_dark_brown"
        />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="icon2, icon3, icon4, icon5" />

    <ImageView
        android:id="@+id/image2"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@color/black"
        android:cropToPadding="true"
        android:scaleType="centerInside"
        app:layout_constraintTop_toBottomOf="@id/hguideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/image3"
        />

    <ImageButton
        android:id="@+id/icon2"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_camera"
        android:backgroundTint="@color/olive_light_green_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/image2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/icon3"
        app:layout_constraintVertical_bias="0.1"
        />


    <ImageView
        android:id="@+id/image3"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@color/black"
        android:cropToPadding="true"
        android:scaleType="centerInside"
        app:layout_constraintTop_toBottomOf="@id/hguideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/image2"
        app:layout_constraintEnd_toStartOf="@id/image4"
        />

    <ImageButton
        android:id="@+id/icon3"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_camera"
        android:backgroundTint="@color/olive_light_green_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/image3"
        app:layout_constraintStart_toEndOf="@id/icon2"
        app:layout_constraintEnd_toStartOf="@id/icon4"
        app:layout_constraintVertical_bias="0.1"
        />

    <ImageView
        android:id="@+id/image4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@color/black"
        android:cropToPadding="true"
        android:scaleType="centerInside"
        app:layout_constraintTop_toBottomOf="@id/hguideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/image3"
        app:layout_constraintEnd_toStartOf="@id/image5"
        />

    <ImageButton
        android:id="@+id/icon4"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_camera"
        android:backgroundTint="@color/olive_light_green_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/image4"
        app:layout_constraintStart_toEndOf="@id/icon3"
        app:layout_constraintEnd_toStartOf="@id/icon5"
        app:layout_constraintVertical_bias="0.1"
        />

    <ImageView
        android:id="@+id/image5"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@color/black"
        android:cropToPadding="true"
        android:scaleType="centerInside"
        app:layout_constraintTop_toBottomOf="@id/hguideline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/image4"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <ImageButton
        android:id="@+id/icon5"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_camera"
        android:backgroundTint="@color/olive_light_green_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/image5"
        app:layout_constraintStart_toEndOf="@id/icon4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.1"
        />



</androidx.constraintlayout.widget.ConstraintLayout>


